{
    "python.testing.pytestArgs": [
        "apps"
    ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    // set fish as default shell
    "terminal.integrated.defaultProfile.linux": "fish",
    "todo-tree.highlights.customHighlight": {
        "BUG": {
            "icon": "bug"
        },
        "HACK": {
            "icon": "tools"
        },
        "FIXME": {
            "icon": "flame",
            "foreground": "#ffffff",
            "background": "#ff0000"
        },
        "XXX": {
            "icon": "x"
        },
        "[ ]": {
            "icon": "issue-draft"
        },
        "[x]": {
            "icon": "issue-closed"
        }
    },
    "cSpell.words": [
        "appname",
        "autoretry",
        "corsheaders",
        "devcontainers",
        "levelname",
        "logoutall",
        "wrongpassword"
    ],
}